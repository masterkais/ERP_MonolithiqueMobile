;\n      //alert(info[0]['id']);\n      if (info && info.status === 200) {\n        var x=info;\n        alert(x.length);\n        if(x.length>0) {\n          alert(x[0]['id']);\n          for (i = 0; i < x.length; i++) {\n\n            prod = new Product();\n            prod.setid(x[i].ID_reg);\n            prod.setname(x[i].DateReg);\n            prod.setdescription(x[i].Montant_reg);\n            prod.setsellingPrice(x[i].IDCLIENT);\n            prod.setbuyingPrice(x[i].NomClt);\n            prod.setstate(x[i].NomClt);\n            prod.setactive(x[i].NomClt);\n            prod.setimagesIds(x[i].NomClt);\n            prod.setcategoryId(x[i].NomClt);\n            prod.setsiteStockId(x[i].NomClt);\n            this.product_list.push(prod);\n\n          }alert(this.product_list[0].getname());\n        }\n      } else {\n       // this.util.showToast(`${info.data.message}`, 'danger', 'bottom');\n      }\n    }).catch(error => {\n      //this.isLogin = false;\n      alert(error);\n      //this.util.showToast(`${error}`, 'danger', 'bottom');\n    });\n  }\n}\n"],"sourceRoot":"webpack:///","file":"pages-ecommerce-example5-example5-module-es5.js"}